{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true, "allow": ["_directories"] }],
        "no-unused-vars": "off",
        "class-methods-use-this": "off"
    },
    "ignorePatterns": ["_test/"]
}
